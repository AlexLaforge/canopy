grammar Lisp
  rule program
    cell+
  end
  
  rule cell
    space* data:(list / atom) space*
  end
  
  rule list
    "(" cell* ")"
  end
  
  rule atom
    boolean / integer / string / symbol
  end
  
  rule boolean
    "#t" / "#f"
  end
  
  rule integer
    [1-9] [0-9]*
  end
  
  rule string
    '"' [^"]* '"'
  end
  
  rule symbol
    (!delimiter .)+
  end
  
  rule space
    [\s\n\r\t]
  end
  
  rule paren
    "(" / ")"
  end
  
  rule delimiter
    paren / space
  end
end

